let num1= Number(prompt('Enter number 1: '));
let num2= Number(prompt('Enter number 2: '));

function calculator(num1,num2){
    function sum(){
        return num1+num2;
    }
    function sub(){
        return num1-num2;
    }
    function mul(){
        return num1*num2;
    }
    function div(){
        return num1/num2;
    }
    return [sum(),sub(),mul(),div()];
}

console.log(calculator(num1,num2));
